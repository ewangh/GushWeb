        <a name="amode" href="javascript:;" onclick="getModeQueue(@Convert.ToInt32(NetbuyMode.散户缩量买入))" value="@Convert.ToInt32(NetbuyMode.散户缩量买入)">@NetbuyMode.散户缩量买入.ToString()</a>[]<span>&nbsp;</span>
        <a name="amode" href="javascript:;" onclick="getModeQueue(@Convert.ToInt32(NetbuyMode.散户缩量卖出))" value="@Convert.ToInt32(NetbuyMode.散户缩量卖出)">@NetbuyMode.散户缩量卖出.ToString()</a>[]<span>&nbsp;</span>
