                <th>
                    @Html.DisplayNameFor(model => model.State)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Time)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Date)
                </th>
            </tr>
        </thead>
        <tbody id="tb">
            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        @Html.Hidden("codes", item.Date == maxDate ? item.Code : "")
                    </td>
                    <td>
                        @ControlHelper.UrlControl(url, item.Code)
                    </td>
                    <td>
                        <a href="javascript:;" onclick="getCodeQueue('@item.Code')" onmouseover = "showImg('@String.Format(img,item.Code)')" >@item.Name</a>
                        @Html.CheckBox("ckb", new { value = @item.Name })
                    </td>
                    <td>
                        @ConvertHelper.ColorConvert(item.Netbuy)
                    </td>
                    <td>
                        <span name="@(item.Code + item.Date)">@ConvertHelper.AmplitudeConvert(item.Change)</span>
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Ltotal)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Num)
                    </td>
                    <td>
                        @ConvertHelper.ForceConvert(item.State)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Time)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Date)
                    </td>
                </tr>
            }
        </tbody>
    </table>
