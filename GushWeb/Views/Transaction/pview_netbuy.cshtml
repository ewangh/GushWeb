<script>
    function getChkNames() {
        var chk_value = [];
        $('input[name="ckb"]:checked').each(function () {
            chk_value.push($(this).val());
        });
        if (chk_value.length > 0) {
            var content = prompt("", chk_value);
            window.clipboardData.setData("Text", content);
        }
    }

    function showImg(url) {
        popup.appendImg(url);
    }
</script>

