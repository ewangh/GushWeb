    <table class="table table-striped">
        <thead>
            <tr>
                <th>

                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Code)
                </th>
                <th>
                    <a href="javascript:;" onclick="getChkNames()">@Html.DisplayNameFor(model => model.Name)</a>
                </th>
                <th>
                    <a href="javascript:;" onclick="getColQueue(1)">@Html.DisplayNameFor(model => model.Netbuy)</a>
                </th>
                <th>
                    <a href="javascript:;" onclick="getColQueue(2)">@Html.DisplayNameFor(model => model.Change)</a>
                </th>
                <th>
                    <a href="javascript:;" onclick="getColQueue(3)">@Html.DisplayNameFor(model => model.Ltotal)</a>
