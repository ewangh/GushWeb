        <a name="amode" href="javascript:;" onclick="getModeQueue(@Convert.ToInt32(NetbuyMode.昨天散户缩量卖出))" value="@Convert.ToInt32(NetbuyMode.昨天散户缩量卖出)">@NetbuyMode.昨天散户缩量卖出.ToString()</a>[]<span>&nbsp;</span>
