@model IEnumerable<t_opennotes>
@foreach (var item in Model)
{
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Rise)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Code)
        </td>
        <td>
            @Html.ActionLink(item.Name, "Details", new { id = item.Code, date = item.CoordDate }, new { target = "_blank", name = item.Code, onmouseover = "showImg('" + item.Code + "')" })
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Date)
        </td>
    </tr>
}
<script>
    function showImg(val) {
        popup.appendImg(val);
    }
</script>
